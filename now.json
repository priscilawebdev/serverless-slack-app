{
  "name": "serverless-slack-app",
  "version": 2,
  "env": {
    "SLACK_CLIENT_ID": "@slack-client-id",
    "SLACK_CLIENT_SECRET": "@slack-client-secret"
  },
  "builds": [
    {
      "src": "*.js",
      "use": "@now/node"
    },
    {
      "src": "*.html",
      "use": "@now/static"
    }
  ]
}
